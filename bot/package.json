{
  "name": "monkey-bot",
  "version": "0.1.0",
  "description": "Discord music bot (discord.js + Shoukaku + Lavalink)",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "node --env-file=.env --enable-source-maps --watch dist/index.js",
    "start": "node --env-file=.env --enable-source-maps dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\""
  },
  "keywords": [
    "discord",
    "music",
    "lavalink",
    "shoukaku"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.1",
    "pino": "^9.9.0",
    "shoukaku": "^4.1.0",
    "undici": "^7.15.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
